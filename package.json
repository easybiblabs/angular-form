    "moment": "2.9.0",
    "objectpath": "1.0.5",
    "pickadate": "3.5.3",
    "tv4": "1.1.9"
  },
  "napa": {
    "angular-summernote": "summernote/angular-summernote#0.2.4",
    "angular-schema-form": "textalk/angular-schema-form#v0.7.3"
  },
  "browser": {
    "angular-summernote": "./node_modules/angular-summernote/dist/angular-summernote.min.js",
    "angular-schema-form": "./node_modules/angular-schema-form/dist/schema-form.js",
    "pickadate/picker": "./node_modules/pickadate/lib/picker.js",
    "pickadate/picker.date": "./node_modules/pickadate/lib/picker.date.js"
  },
  "browserify-shim": {
    "pickadate/picker": {
      "depends": [
        "jquery"
      ]
    },
