{
  "name": "angular-form",
  "version": "0.0.5",
  "description": "A module to provide angular form validation using json validator with the help of angular-schema-form. We have added some adjustments like * invalidate any formfield beside json validation to be able to display backend errors. * map doctrine backend validation to json schema validation ( enables to add constraints to entities in backend , and validate the form in javascript using the same constraints)",
  "main": "src/index.js",
  "scripts": {
    "napa": "napa",
    "install": "napa"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/easybiblabs/angular-form.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/easybiblabs/angular-form/issues"
  },
  "homepage": "https://github.com/easybiblabs/angular-form",
  "devDependencies": {
    "brfs": "1.4.0",
    "browserify": "9.0.3",
    "browserify-shim": "3.8.3",
    "chai": "2.1.0",
    "gulp": "3.8.11",
    "gulp-angular-templatecache": "1.5.0",
    "gulp-concat": "2.5.2",
    "gulp-eslint": "0.2.0",
    "gulp-jscs": "1.3.1",
    "gulp-minify-html": "1.0.0",
    "karma-browserify": "2.0.0",
    "karma-mocha": "0.1.9",
    "karma-phantomjs-launcher": "0.1.4",
    "karma-sinon": "1.0.4",
    "karma-sinon-chai": "0.2.0",
    "napa": "1.2.0",
    "vinyl-source-stream": "1.0.0"
  },
  "dependencies": {
    "angular": "^1.3.8",
    "angular-loading-bar": "0.6.*",
    "angular-sanitize": "1.3.14",
    "angular-mocks": "1.3.0",
    "angular-translate": "2.6.0",
    "angular-schema-form": "0.7.13",
    "bootstrap": "3.3.2",
    "jquery": "2.1.1",
    "moment": "2.9.0",
    "objectpath": "1.1.0",
    "pickadate": "3.5.3",
    "select2": "3.5.2-browserify",
    "summernote": "0.5.9",
    "tv4": "1.0.15"
  },
  "napa": {
    "angular-summernote": "summernote/angular-summernote#0.2.4"
  },
  "browser": {
    "ngSanitize": "./node_modules/angular-sanitize/angular-sanitize.js",
    "angular-summernote": "./node_modules/angular-summernote/dist/angular-summernote.min.js",
    "angular-schema-form": "./node_modules/angular-schema-form/dist/schema-form.min.js",
    "pickadate/picker": "./node_modules/pickadate/lib/picker.js",
    "pickadate/picker.date": "./node_modules/pickadate/lib/picker.date.js",
    "summernote": "./node_modules/summernote/dist/summernote.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "tv4": {
      "exports": "tv4"
    },
    "angular": {
      "exports": "angular"
    },
    "angular-schema-form": {
      "depends": [
        "moment",
        "objectpath:ObjectPath",
        "tv4:tv4"
      ]
    },
    "objectpath": {
      "exports": "global:ObjectPath"
    },
    "moment": {
      "exports": "moment"
    },
    "./utility/summernote-lang": {
      "depends": "summernote:$"
    },
    "summernote": {
      "depends": [
        "bootstrap",
        "jquery:$",
        "angular-summernote"
      ]
    },
    "angular-summernote": {
      "depends": [
        "angular",
        "summernote"
      ]
    }
  }
}
