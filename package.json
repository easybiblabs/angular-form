      ]
    },
    "jquery": {
      "exports": "$"
    },
    "tv4": {
      "exports": "tv4"
    },
    "pickadate": {
      "depends": [
        "jquery"
      ]
    },
    "angular": {
      "exports": "angular"
    },
    "objectpath": {
      "exports": "ObjectPath"
    },
    "moment": {
      "exports": "moment"
    },
    "bootstrap": {
      "depends": [
        "jquery"
      ]
    },
    "summernote": {
      "depends": [
        "bootstrap",
        "jquery"
      ]
    },
    "angular-summernote": {
      "depends": [
        "angular",
        "summernote"
      ]
    },
    "angular-schema-form": {
      "depends": [
        "tv4",
        "objectpath"
      ]
    }
